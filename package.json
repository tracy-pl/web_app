{
  "name": "web_app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f environments/.env.local react-scripts start",
    "start:build": "serve -s build",
    "build": "react-scripts build",
    "build:devci": "env-cmd -f environments/.env.devci yarn build",
    "build:prod": "env-cmd -f environments/.env.prod yarn build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@reduxjs/toolkit": "1.9.1",
    "antd": "5.1.1",
    "async-mutex": "0.4.0",
    "axios": "1.2.1",
    "fontfaceobserver": "2.3.0",
    "i18next": "22.4.6",
    "i18next-browser-languagedetector": "7.0.1",
    "i18next-scanner": "4.1.0",
    "inquirer": "9.1.4",
    "inquirer-directory": "2.2.0",
    "lodash": "4.17.21",
    "nestjs-command": "3.1.2",
    "node-plop": "0.31.0",
    "plop": "3.1.1",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "12.1.1",
    "react-is": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.6.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "18.2.0",
    "redux-persist": "6.0.0",
    "serve": "14.1.2",
    "shelljs": "0.8.5",
    "styled-components": "5.3.6",
    "web-vitals": "3.1.0",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/fontfaceobserver": "2.1.0",
    "@types/jest": "29.2.4",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-redux": "7.1.24",
    "@types/react-test-renderer": "18.0.0",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.11",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.18.0",
    "chalk": "5.2.0",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "stylelint": "14.16.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14.0",
    "yarn": ">=1.22.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
